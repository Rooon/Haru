        <TextBox x:Name="boxMouseX" HorizontalAlignment="Left" Height="23" Margin="75,52,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="boxMouseY" HorizontalAlignment="Left" Height="23" Margin="75,80,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
